{
  "name": "over-scrap",
  "version": "1.2.0",
  "description": "playoverwatch.com data scrapper to get all the stats from a player profile into a JSON object",
  "homepage": "https://github.com/hoshin/OverScrap",
  "bugs": {
    "url": "https://github.com/hoshin/OverScrap/issues"
  },
  "main": "dist/overScrap.min.js",
  "scripts": {
    "test": "mocha --compilers js:babel-core/register --recursive ./test ",
    "lint": "./node_modules/.bin/eslint .",
    "start": "./bin/overscrap-cli.js",
    "build": "babel ./index.js -o ./dist/overScrap.js && ./node_modules/uglify-js/bin/uglifyjs ./dist/overScrap.js -c -m -o ./dist/overScrap.min.js",
    "coverage": "istanbul cover _mocha --dir qualimetry/coverage -- --compilers js:babel-core/register -R spec --recursive",
    "publish-patch": "npm version patch && git push && npm publish",
    "publish-minor": "npm version minor && git push && npm publish",
    "publish-major": "npm version major && git push && npm publish",
    "preversion": "npm run lint && npm test",
    "version": "npm run build && git add dist/overScrap.min.js",
    "postversion": "git push && git push --tags"
  },
  "author": "Martin Bahier",
  "license": "MIT",
  "keywords": [
    "overwatch",
    "statistics",
    "scrapper"
  ],
  "dependencies": {
    "bluebird": "^3.5.0",
    "cheerio": "^1.0.0-rc.1",
    "lodash": "^4.17.4",
    "request": "^2.81.0",
    "request-promise": "^4.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.0.1",
    "eslint": "^3.19.0",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^3.4.2",
    "sinon": "^2.3.2",
    "uglify-js": "^3.0.15"
  }
}
